def solution(s):
    check = 0
    for i in s:
        if i == ')':
            check += -1
            if check < 0:
                break
        elif i == '(':
            check += 1
    if check == 0:
        return True
    else:
        return False


def is_pair(s):
    # 함수를 완성하세요
    x = 0
    for w in s:
        if x < 0:
            break
        x = x+1 if w=="(" else x-1 if w==")" else x
    return x==0
